{
    "version": 1.0,
    "formatters": {
        "simple": {
            "format": "%(asctime)s [%(levelname)-7s] %(name)s : %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "DEBUG",
            "handlers": ["console"]
        }
    }
}